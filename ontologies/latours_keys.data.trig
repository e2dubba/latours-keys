@prefix ant:<https://e2dubba.github.io/latours-keys/ontology/v1#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix dbo: <https://dbpedia.org/ontology/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix gist: <https://ontologies.semanticarts.com/gist/> .

@prefix : <https://e2dubba.github.io/latours-keys/data#> .

ant:Actant
  a owl:Class
  ; a owl:NamedIndividual
  ; skos:definition "Both Human and and Non-Human individuals that interact in the system"@en
  ; skos:prefLabel "Actor/Actant"@en
  .

## Human Actors
ant:HumanActor
  a owl:Class
  ; rdfs:subClassOf ant:Actant
  ; skos:definition "A Human Actor in the system"@en
  ; skos:prefLabel "Human Actor"@en
  .

ant:HotelManager
  a owl:Class
  ; rdfs:subClassOf ant:Actant
  ; rdfs:subClassOf gist:Person
  ; skos:definition "A person who has a program, and wants to inact it"@en
  ; skos:editorialNote "I think this might should be more general. For all those who want to enact a program?"@en
  ; skos:prefLabel "Hotel Manager"@en
  .

ant:Customer
  a owl:Class
  ; rdfs:subClassOf gist:Person
  ; rdfs:subClassOf ant:HumanActor
  ; skos:definition "A person who is renting a room in the hotel"@en
  .

ant:Innovator
  a owl:Class
  ; rdfs:subClassOf gist:Person
  ; rdfs:subClassOf ant:HumanActor
  ; skos:definition "Some one who creates, fabricates, or adapts materials"@en
  .


## Non-Human Actants

ant:NonHumanActant
  a owl:Class
  ; rdfs:subClassOf ant:Actant
  ; skos:definition "Non-Human Actant"@en
  ; skos:prefLabel "Non Human Actant"
  .

ant:HotelRoom
  a owl:Class
  ; rdfs:subClassOf ant:NonHumanActant
  ; skos:definition "A hotel room intended to be rented out"@en
  ; skos:prefLabel "Hotel Room"@en
  .

ant:Key
  a owl:Class
  ; rdfs:subClassOf ant:NonHumanActant
  ; skos:definition "an instrument used to an unlock a door"@en
  .

ant:Message
  a owl:Class
  ; rdfs:subClassOf ant:NonHumanActant
  ; rdfs:comment "A linguistic communictation of the program"@en
  ; skos:prefLabel "Message"@en
  .

ant:Sign
  a owl:Class
  ; rdfs:subClassOf ant:NonHumanActant
  ; skos:definition "a display containing a message"@en
  .

ant:Weight
  a owl:Class
  ; rdfs:subClassOf ant:NonHumanActant
  ; skos:definition "the weight attached to a key"@en
  .

# Origin info

ant:LanguageFluency
  a owl:Class
  ; skos:definition "Defines the level of knowlege an actor has about a specified language"@en
  ; skos:prefLabel "Language Fluency"@en
  .

# The Program
ant:Program
  a owl:Class
  ; rdfs:comment "How would Latour define Program? The course of action an actor places on other actors in the system?"@en
  .

ant:ProgramDisposition
  a owl:Class
  ; skos:definition "The way an actor relates to a program"@en
  ; skos:prefLabel "Program Disposition"@en
  .

ant:Intention
  a owl:Class
  ; skos:definition "The goal is the ideal state of the program, which actors and which actants, current behavior, and target behavior, locations"@en
  ; skos:prefLabel "Intention"@en
  .

ant:State
  a owl:Class
  ; skos:definition "The final state, position, location or mood the program is hoping to achieve"@en
  ; skos:prefLabel "State"@en
  .

## Attitudes and Dispositions (especially in regard to the program)

ant:Mood
  a owl:Class
  ; owl:subClassOf ant:State
  ; rdfs:comment "The different types of dispositions that impact a way a human actor relates to a program"@en
  ; rdfs:comment "the specific moods are actually instances and not subclasses"
  ; skos:prefLabel "Mood"@en
  .

ant:Position
  a owl:Class
  ; owl:subClassOf ant:State
  ; rdfs:comment "this is the different positions a state can be in for an object, e.g. locked or unlocked"@en
  ; skos:prefLabel "Position"@en
  .

:_OnHotelDesk
  a ant:State
  ; a gist:GeoLocation # This needs to be place that is related to but not overlapping with state
  ; skos:prefLabel "Hotel Desk"@en
  .

:_Room1
  a ant:HotelRoom
  ; ant:hasKey :_HotelKey_1
  ; skos:prefLabel "Room 1"@en
  .

:_Room2
  a ant:HotelRoom
  ; ant:hasKey :_HotelKey_2
  ; skos:prefLabel "Room 2"@en
  .

:_HotelKey_1
  a ant:Key
  ; ant:fitsRoom :_Room1
  ; skos:prefLabel "Key 1"@en
  ; ant:connectedTo [
    a ant:Weight
    ; ant:hasMassInKg 0.1
  ]
  .

:_HotelKey_2
  a ant:Key
  ; ant:fitsRoom :_Room2
  ; skos:prefLabel "Key 2"@en
  ; ant:connectedTo [
    a ant:Weight
    ; ant:hasMassInKg 0.1
  ]
  .

:_ReturnKeyMessage
  a ant:Message
  ; skos:prefLabel "Please Return Message"@en
  ; schema:inLanguage :_GermanLanguage
  ; schema:inLanguage :_EnglishLanguage
  ; schema:inLanguage :_DutchLanguage
  ; schema:inLanguage :_FrenchLanguage
  ; gist:containedText "Please return hotel key to front desk when you leave for the day"@en
  ; gist:containedText "Sie den Hotelschlüssel an der Rezeption ab, wenn Sie für den Tag abreisen"@de
  .


:_PleaseReturnSign
  a ant:Sign
  ; skos:prefLabel "Please Return Sign"@en
  ; ant:hasMessage :_ReturnKeyMessage
  .

:_ReturnKeysToDesk
  a ant:Intention
  ; ant:fromPerspectiveOf :_PierreTheManager
  ; ant:concernedAbout :_HotelKey_1
  ; ant:concernedAbout :_HotelKey_2
  # ; ant:concernedAbout :_HotelKey_3
  # ; ant:concernedAbout :_HotelKey_4
  # ; ant:concernedAbout :_HotelKey_5
  # ; ant:concernedAbout :_HotelKey_6
  # ; ant:concernedAbout :_HotelKey_7
  ; ant:desiredEndState :_OnHotelDesk
  .

:_PierresKeys
  a ant:Program
  ; ant:hasIntention :_ReturnKeysToDesk
  ; ant:hasAdherent :_PierreTheManager
  ; ant:hasAdherent :_PleaseReturnSign
  # once weights are added to the keys, the keys become adherents to the program
  # ; ant:hasAdherent :_HotelKey_1
  # ; ant:hasAdherent :_HotelKey_2
  ; ant:hasVersion 1
  .

:_Savagery
  a ant:Mood
  ; rdfs:comment "An actors predisposition to the savagery, especially as it relates to a given program"@en
  ; skos:prefLabel "Savagery"@en
  .

:_Carelessness
  a ant:Mood
  ; rdfs:comment "An actors predisposition to carelessness, especially as it relates to a given program"@en
  ; skos:prefLabel "Carelessness"@en
  .

:_Willingness
  a ant:Mood
  ; rdfs:comment "An actors willingness to go with another actors program"@en
  ; skos:prefLabel "Willingness"@e
  ; skos:editorialNote "Perhaps there should be a inverse relationship to resistence, but I think marking willingness is helpful enough"@en
  .

